{
	"name": "qualtrics-thiscovery-skin",
	"version": "3.0.0",
	"latest_alias": "2.0.0",
	"description": "Javascript and CSS to customise Qualtrics surveys",
	"main": "./dist/thiscovery-skin.js",
	"scripts": {
		"build": "run-s build:scss build:postcss build:js build:copy_latest",
		"build:js": "esbuild ./src/v$npm_package_version/js/skin.js --bundle --minify --sourcemap --loader:.md=text --outfile=./dist/bundle.$npm_package_version.js --target=es2016,chrome94,firefox94,safari13,edge96,node12",
		"build:scss": "sass ./src/v$npm_package_version/css/skin.scss ./src/v$npm_package_version/css/skin.css --embed-sources",
		"build:copy_latest": "cp ./dist/bundle.$npm_package_version.css ./dist/bundle.latest.css & cp ./dist/bundle.$npm_package_version.js ./dist/bundle.latest.js",
		"build:postcss": "node ./css_postprocess.js",
		"dev": "node dev.js",
		"dev_tasks": "run-p watch:js watch:scss",
		"doiuse": "npx doiuse ./src/v$npm_package_version/css/skin.css",
		"test": "echo \"Error: no test specified\" && exit 1",
		"skinjob": "node ./tools/skinjob/skinjob.js ./src/v$npm_package_version/css/skin.css --novalidation",
		"watch:js": "chokidar --verbose \"src/**/*.js\" -c \"npm run build:js\"",
		"watch:scss": "chokidar --verbose \"src/v$npm_package_version/css/**/*.scss\" -c \"npm run build:scss\"",
		"watch:spesh3": "chokidar --verbose \"src/v2.1.0/css/**/*.scss\" -c \"npm run build:scss\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/THIS-Institute/qualtrics-thiscovery-skin.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/THIS-Institute/qualtrics-thiscovery-skin/issues"
	},
	"homepage": "https://github.com/THIS-Institute/qualtrics-thiscovery-skin#readme",
	"devDependencies": {
		"autoprefixer": "^10.4.2",
		"browserslist": "^4.19.1",
		"chalk": "^3.0.0",
		"chokidar": "^3.5.3",
		"chokidar-cli": "^3.0.0",
		"cli-table": "^0.3.11",
		"command-line-args": "^5.2.1",
		"command-line-usage": "^6.1.1",
		"css-validator": "^0.10.0",
		"cssnano": "^5.0.17",
		"doiuse": "^4.4.1",
		"enquirer": "^2.3.6",
		"esbuild": "^0.14.22",
		"express": "^4.17.2",
		"npm-run-all": "^4.1.5",
		"papaparse": "^5.3.2",
		"postcss": "^8.4.6",
		"postcss-cli": "^9.1.0",
		"postcss-discard": "^2.0.0",
		"postcss-use": "^3.0.0",
		"sass": "^1.49.7",
		"socket.io": "^4.4.1"
	},
	"dependencies": {
		"a11y-dialog": "^7.4.0",
		"blissfuljs": "^1.0.6",
		"css-reset-and-normalize": "^2.3.6",
		"lodash": "^4.17.21",
		"markdown": "^0.5.0",
		"socket.io-client": "^4.4.1",
		"util": "^0.12.4"
	},
	"browserslist": "> 0.5% in GB"
}